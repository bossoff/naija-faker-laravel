{
  "name": "kodegrenade/naija-faker-laravel",
  "keywords": [
    "php",
    "github",
    "laravel",
    "faker",
    "naija-faker",
    "package",
    "Open Source"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Temitope Ayotunde",
      "email": "brhamix@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Kodegrenade\\NaijaFaker\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Kodegrenade\\NaijaFaker\\NaijaFakerServiceProvider"
      ],
      "aliases": {
        "NaijaFaker": "Kodegrenade\\NaijaFaker\\NaijaFaker"
      }
    }
  },
  "description": "A simple package that generates fake typical Nigerian data.",
  "require-dev": {
    "phpunit/phpunit": "^8.4|^10.0",
    "illuminate/support": "^11.0@dev",
    "pestphp/pest": "2.x-dev",
    "pestphp/pest-plugin-drift": "2.x-dev"
  },
  "autoload-dev": {
    "psr-4": {
      "Kodegrenade\\NaijaFaker\\Test\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/pest"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/kodegrenade/naija-faker-laravel"
    }
  ]
}